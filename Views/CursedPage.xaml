    </Page.Resources>
    <StackPanel HorizontalAlignment="Center" >
        <muxc:NavigationView x:Name="nv" Canvas.ZIndex="2" SelectionChanged="nv_SelectionChanged" PaneDisplayMode="Top" Background="#323232" IsSettingsVisible="False" IsBackButtonVisible="Collapsed">
            <muxc:NavigationView.MenuItems>
                <muxc:NavigationViewItem Content="File" x:Name="File"/>
                <muxc:NavigationViewItem Content="Home" x:Name="Home"/>
                <muxc:NavigationViewItem Content="Annotation" Visibility="Collapsed"/>
                <muxc:NavigationViewItem Content="Ink" x:Name="Ink"/>
                <muxc:NavigationViewItem Content="View" x:Name="View"/>
            </muxc:NavigationView.MenuItems>
            <muxc:NavigationView.PaneHeader>
                <Grid HorizontalAlignment="Left">
                    <Button Background="Transparent" Margin="20,0,0,0" Name="BackButton" Style="{StaticResource PdfViewerBackBtnStyle}">
                    </Button>
                </Grid>
            </muxc:NavigationView.PaneHeader>
            <muxc:NavigationView.PaneFooter>
                <StackPanel Orientation="Horizontal">
                    <Grid   HorizontalAlignment="Center">
                        <Button Background="Transparent" Name="UndoButton" Style="{StaticResource PdfViewerUndoBtnStyle}" Command="{Binding ElementName=pdfViewer, Path=UndoCommand, Mode=TwoWay}">
                        </Button>
                    </Grid>
                    <Grid Margin="2" HorizontalAlignment="Center">
                        <Button Background="Transparent" Name="RedoButton" Style="{StaticResource PdfViewerRedoBtnStyle}" Command="{Binding ElementName=pdfViewer, Path=RedoCommand, Mode=TwoWay}" >
                        </Button>
                    </Grid>
                    <SearchBox x:Name="SearchBox" 
    FocusOnKeyboardInput="True"
                               Width="300"
                               Margin="4"
                               QuerySubmitted="SearchBox_QuerySubmitted"
    Height="30"/>
                    <CommandBar OverflowButtonVisibility="Collapsed" DefaultLabelPosition="Right" Background="Transparent">
                        <AppBarButton  CornerRadius="5,0 ,10, 0" Click="PrevButton_Click">
                            <AppBarButton.Icon>
                                <FontIcon FontFamily="Segoe MDL2 Assets" Glyph="&#xE70E;"/>
                            </AppBarButton.Icon>
                        </AppBarButton>
                        <AppBarButton  CornerRadius="5,0 ,10, 0" Click="NextButton_Click">
                            <AppBarButton.Icon>
                                <FontIcon FontFamily="Segoe MDL2 Assets" Glyph="&#xE70D;"/>
                            </AppBarButton.Icon>
                        </AppBarButton>
                    </CommandBar>
                </StackPanel>
            </muxc:NavigationView.PaneFooter>
            <Grid>
                <StackPanel Orientation="Horizontal" x:Name="InkBar" Background="Transparent">
                    <CommandBar OverflowButtonVisibility="Collapsed" DefaultLabelPosition="Right" Background="Transparent">
                        <AppBarButton Label="Ink" Click="InkButton_Click">
                            <AppBarButton.Icon>
                                <FontIcon FontFamily="Segoe MDL2 Assets" Glyph="&#xEDC6;"/>
                            </AppBarButton.Icon>
                        </AppBarButton>
                        <AppBarButton Label="Color">
                            <AppBarButton.Icon>
                                <FontIcon FontFamily="Segoe MDL2 Assets" Glyph="&#xE790;"/>
                            </AppBarButton.Icon>
                            <AppBarButton.Flyout>
                                <Flyout>
                                    <hsvColorPickers:ColorPicker x:Name="ColorPicker" Background="Transparent" ColorChanged="ColorPicker_ColorChangeCompleted" ColorChangeCompleted="ColorPicker_ColorChangeCompleted"/>
                                </Flyout>
                            </AppBarButton.Flyout>
                        </AppBarButton>
                    </CommandBar>
                    <TextBlock Margin="5, 0, 3, 0" VerticalAlignment="Center" Text="Size:"/>
                    <Slider x:Name="ThiccSlider" ValueChanged="ThiccSlider_ValueChanged" Margin="5" Width="200" Minimum="1" Maximum="50" Value="10"/>
                    <TextBlock VerticalAlignment="Center" Text="Opacity:"/>
                    <Slider x:Name="OpacitySlider" ValueChanged="OpacitySlider_ValueChanged" Margin="5" Width="100" Minimum="0" Maximum="1" Value="1" StepFrequency="0.05"/>
                    <TextBlock VerticalAlignment="Center" Text="Mode:" Margin="4, 0,4, 0"/>
                    <ToggleSwitch x:Name="InkNative" Toggled="InkNative_Toggled" OffContent="Low performance" OnContent="High performance" IsOn="True" />
                </StackPanel>
                <CommandBar OverflowButtonVisibility="Collapsed" DefaultLabelPosition="Right" x:Name="FileBar" Visibility="Collapsed" Background="#323232">
                <AppBarButton Label="Open PDF" Icon="OpenFile" Click="OpenButton_Click"></AppBarButton>
                    <AppBarButton Label="Print" Icon="Print" Command="{Binding PrintCommand}"></AppBarButton>
                </CommandBar>
                <StackPanel Orientation="Horizontal" x:Name="HomeBar" Background="Transparent">
                    <CommandBar OverflowButtonVisibility="Collapsed" DefaultLabelPosition="Right" Background="Transparent">
                        <AppBarButton Label="Previous" Command="{Binding ElementName=pdfViewer, Path=PreviousPageCommand}">
                            <AppBarButton.Icon>
                                <FontIcon FontFamily="Segoe MDL2 Assets" Glyph="&#xE70E;"/>
                            </AppBarButton.Icon>
                        </AppBarButton>
 
                        <AppBarButton Label="Next" Command="{Binding ElementName=pdfViewer, Path=NextPageCommand}" >
                            <AppBarButton.Icon>
                                <FontIcon FontFamily="Segoe MDL2 Assets" Glyph="&#xE70D;"/>
                            </AppBarButton.Icon>
                        </AppBarButton>
                        <AppBarSeparator/>
                        <AppBarButton CornerRadius="5,0 ,10, 0" Label="First page" Command="{Binding ElementName=pdfViewer, Path=FirstPageCommand}">
                            <AppBarButton.Icon>
                                <FontIcon FontFamily="Segoe MDL2 Assets" Glyph="&#xE74A;"/>
                            </AppBarButton.Icon>
                        </AppBarButton>
                        <AppBarButton Label="Last page" Command="{Binding ElementName=pdfViewer, Path=LastPageCommand}">
                            <AppBarButton.Icon>
                                <FontIcon FontFamily="Segoe MDL2 Assets" Glyph="&#xE74B;"/>
                            </AppBarButton.Icon>
                        </AppBarButton>
                        <AppBarSeparator/>
                    </CommandBar>

                </StackPanel>
                <StackPanel Orientation="Horizontal" x:Name="ViewBar" Visibility="Collapsed" Background="#323232">
                    <CommandBar OverflowButtonVisibility="Collapsed" DefaultLabelPosition="Right" Background="Transparent">
                        <AppBarButton Label="View" Click="ViewButton_Click">
                            <AppBarButton.Icon>
                                <FontIcon FontFamily="Segoe MDL2 Assets" Glyph="&#xEA90;"/>
                            </AppBarButton.Icon>
                        </AppBarButton>
                        <AppBarButton Label="Fit" Click="FitViewButton_Click">
                            <AppBarButton.Icon>
                                <FontIcon FontFamily="Segoe MDL2 Assets" Glyph="&#xE9A6;"/>
                            </AppBarButton.Icon>
                        </AppBarButton>
                        <AppBarButton Label="One" Click="OneViewButton_Click">
                            <AppBarButton.Icon>
                                <FontIcon FontFamily="Segoe MDL2 Assets" Glyph="&#xF56E;"/>
                            </AppBarButton.Icon>
                        </AppBarButton>
                        <AppBarSeparator/>
                        <AppBarButton Label="Zoom in" Icon="ZoomIn"  Command="{Binding ElementName=pdfViewer, Path=IncreaseZoomCommand}"></AppBarButton>
                        <AppBarButton Label="Zoom out" Icon="ZoomOut" Command="{Binding ElementName=pdfViewer, Path=DecreaseZoomCommand}" ></AppBarButton>
                    </CommandBar>
                </StackPanel>
            </Grid>
        </muxc:NavigationView>

        <Grid Background="{ThemeResource SystemControlAcrylicWindowBrush}">

            <Hub Header="Open a pdf">
                <HubSection MinWidth="400" Header="Recents" Background="#242424">
                        <DataTemplate>
                            <StackPanel>
                                <TextBlock Text="The most recently opened files will be here" 
                   Style="{ThemeResource BodyTextBlockStyle}"/>
                            <Grid Grid.Row="4" VerticalAlignment="Stretch" >
                                <Button x:Name="GlobalOpenButton"  Background="Transparent" Tag="Open" Style="{StaticResource PdfViewerGlobalOpenBtnStyle}"  Click="OpenButton_Click">
                                </Button>
                            </Grid>
                        </StackPanel>
                        </DataTemplate>
                    </HubSection>

                <HubSection x:Name="Tech" Header="Settings" IsHeaderInteractive="False"  
        Background="#191919" MinWidth="250">
                    <DataTemplate>
                        <StackPanel>
                            <TextBlock Text="Setting" 
                   Style="{ThemeResource BodyTextBlockStyle}"/>
                            <Button x:Name="Button3" Margin="5"
   CornerRadius="5" Padding="4,-3,4,-3" BorderThickness="1" FontWeight="Bold">
                                <Button.Resources>
                                    <ResourceDictionary>
                                        <ResourceDictionary.ThemeDictionaries>
                                            <ResourceDictionary x:Key="Default">
                                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1" x:Key="ButtonBackground">
                                                    <GradientStop Color="Aqua" Offset="0.0"/>
                                                    <GradientStop Color="Blue" Offset="1.0"/>
                                                </LinearGradientBrush>
                                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1" x:Key="ButtonBorderBrush">
                                                    <GradientStop Color="White" Offset="0.0"/>
                                                    <GradientStop Color="Purple" Offset="1.0"/>
                                                </LinearGradientBrush>

                                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1" x:Key="ButtonBackgroundPointerOver">
                                                    <GradientStop Color="#bfd5407a" Offset="0.0"/>
                                                    <GradientStop Color="#bffa7765" Offset="1.0"/>
                                                </LinearGradientBrush>
                                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1" x:Key="ButtonBorderBrushPointerOver">
                                                    <GradientStop Color="Purple" Offset="0.0"/>
                                                    <GradientStop Color="White" Offset="1.0"/>
                                                </LinearGradientBrush>

                                                <SolidColorBrush x:Key="ButtonForeground" Color="White"/>
                                                <SolidColorBrush x:Key="ButtonForegroundPointerOver" Color="White"/>
                                                <SolidColorBrush x:Key="ButtonForegroundPressed" Color="White"/>
                                                <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="#bfd5407a"/>
                                            </ResourceDictionary>
                                            <ResourceDictionary x:Key="Dark">
                                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1" x:Key="ButtonBackground">
                                                    <GradientStop Color="Aqua" Offset="0.0"/>
                                                    <GradientStop Color="Blue" Offset="1.0"/>
                                                </LinearGradientBrush>
                                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1" x:Key="ButtonBorderBrush">
                                                    <GradientStop Color="White" Offset="0.0"/>
                                                    <GradientStop Color="#bfdd378c" Offset="1.0"/>
                                                </LinearGradientBrush>

                                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1" x:Key="ButtonBackgroundPointerOver">
                                                    <GradientStop Color="#bf8737dd" Offset="0.0"/>
                                                    <GradientStop Color="#bf425ff4" Offset="1.0"/>
                                                </LinearGradientBrush>
                                                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1" x:Key="ButtonBorderBrushPointerOver">
                                                    <GradientStop Color="#bfdd378c" Offset="0.0"/>
                                                    <GradientStop Color="White" Offset="1.0"/>
                                                </LinearGradientBrush>

                                                <SolidColorBrush x:Key="ButtonForeground" Color="White"/>
                                                <SolidColorBrush x:Key="ButtonForegroundPointerOver" Color="White"/>
                                                <SolidColorBrush x:Key="ButtonForegroundPressed" Color="White"/>
                                                <SolidColorBrush x:Key="ButtonBackgroundPressed" Color="#bf8737dd"/>
                                            </ResourceDictionary>
                                        </ResourceDictionary.ThemeDictionaries>
                                    </ResourceDictionary>
                                </Button.Resources>

                                <StackPanel Orientation="Horizontal">
                                    <FontIcon Glyph="&#xE946;" Margin="0,10,0,10" FontSize="15" FontWeight="Bold"/>
                                    <TextBlock Text="About" Margin="10, 7, 0, 0"/>
                                </StackPanel>
                            </Button>
                            <notification:SfHubTile x:Name="hubTile" Foreground="White" 

RotationDepth="40" Width="150" Height="150"

Content="RATE IT ON STORE"

Header="RATE OUR APP" />

                        </StackPanel>
                    </DataTemplate>
                </HubSection>
            </Hub>
                <syncfusion:SfPdfViewerControl x:Name="pdfViewer" Canvas.ZIndex="1" Height="1000" HorizontalAlignment="Right" ShowPageNumber="False"></syncfusion:SfPdfViewerControl>
        </Grid>
    </StackPanel>
</Page>
